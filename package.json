{
  "name": "face-mask",
  "version": "1.0.0",
  "description": "A browser extension that puts masks on faces on the internet.",
  "private": true,
  "repository": "https://github.com/moklick/face-mask-browser-extension.git",
  "author": "moklick",
  "license": "MIT",
  "engines": {
    "node": ">=10 <=12"
  },
  "scripts": {
    "dev:chrome": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch",
    "dev:firefox": "cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build": "yarn run build:chrome && yarn run build:firefox"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "face-api.js": "^0.22.2",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-onepass": "^2.1.3",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.3",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.0.0",
    "webpack-extension-reloader": "^1.1.4",
    "wext-manifest": "^2.2.0",
    "write-webpack-plugin": "^1.1.0",
    "zip-webpack-plugin": "^3.0.0"
  }
}
